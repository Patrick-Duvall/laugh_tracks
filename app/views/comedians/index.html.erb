<div id="statistics">

  <p>Comedians are from: <% @comedians.each do |comedian|  %>
  <%= "#{comedian.city} \n"  %>

  <% end %> </p>

</div>

<% @comedians.each do |comedian| %>
<section id="comedian-<%= comedian.id %>">
  <%= comedian.name %>
  <%= comedian.age %>
  <%= comedian.city %>
  <%= comedian.specials.count %>
  <img src=<%= comedian.image %> >
  <% comedian.specials.each do |special| %>
  <%= special.name %>
  <%= special.runtime_mins %>

  <% end %>
  </section>
<% end %>
